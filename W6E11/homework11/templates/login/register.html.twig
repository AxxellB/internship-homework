{% extends 'base.html.twig' %}

{% block title %}Log in!{% endblock %}

{% block body %}
    <div class="container">
    <form method="post">
        <div class="container">
            <h1>Register</h1>

            {{ form_start(form) }}
            {{ form_end(form) }}
            <input type="hidden" name="_csrf_token"
                   value="{{ csrf_token('authenticate') }}"
            >
            <button type="submit" class="btn btn-primary">Register</button>

        </div>
    </form>
    </div>
{% endblock %}
